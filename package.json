{
  "name": "vite-electron",
  "version": "0.1.0",
  "description": "A vite electron template",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "vite",
    "bd": "vite build && electron-builder",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "prebuild-install": "electron-rebuild -f -w better-sqlite3"
  },
  "keywords": [
    "vite3",
    "electron"
  ],
  "author": "Hunlongyu <hunlongyu@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "electron": "^19.0.11",
    "electron-builder": "^23.1.0",
    "electron-rebuild": "^3.2.8",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-vue": "^9.3.0",
    "naive-ui": "^2.31.0",
    "sass": "^1.54.0",
    "unplugin-auto-import": "^0.10.1",
    "unplugin-vue-components": "^0.21.2",
    "vite": "^3.0.3",
    "vite-plugin-electron": "^0.8.3",
    "vite-plugin-electron-renderer": "^0.8.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.39.0"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "better-sqlite3": "^7.6.2",
    "ffi-cross": "^5.0.11",
    "knex": "^2.2.0"
  }
}
